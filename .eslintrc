{
  "env": {
      "browser": true,
      "mocha": true,
      "node": true
  },
  "extends": "airbnb",
  "rules": {
      "arrow-parens": ["error", "as-needed"],
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "comma-dangle": 0,
      "react/prop-types": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/jsx-indent": [2, 4],
      "react/jsx-indent-props": [2, 4],
      "no-nested-ternary": 0,
      "no-confusing-arrow": 0
  },
  "settings": {
      "import/resolver": {
          "babel-module": {}
      }
  },
  "plugins": [
    "react"
  ]
}
